{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Resource-Policy",
          "value": "same-origin"
        }
      ]
    }
  ],

  "rewrites": [
    {
      "source": "/:path*\\.(html|js|css|png|jpg|jpeg|svg|webp|ico|wasm|json)",
      "destination": "/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}